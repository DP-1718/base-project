 --><beans xmlns="http://www.springframework.org/schema/beans" xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd     http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd    ">
	<!-- 
		Add your own converters using entries of the following form: 
		<bean class="converters.Class1ToClass2" /> 
	-->
	<util:list id="converters">
		<bean class="converters.ConfigurationToStringConverter"/>
		<bean class="converters.StringToConfigurationConverter"/>
		
        <!--Autogenerated converters-->

		<!--Actor converters-->
		<bean class="converters.StringToActorConverter"/>
        <bean class="converters.ActorToStringConverter"/>

		<!--Administrator converters-->
		<bean class="converters.StringToAdministratorConverter"/>
        <bean class="converters.AdministratorToStringConverter"/>

		<!--Message converters-->
		<bean class="converters.StringToMessageConverter"/>
        <bean class="converters.MessageToStringConverter"/>

		<!--MessageFolder converters-->
		<bean class="converters.StringToMessageFolderConverter"/>
        <bean class="converters.MessageFolderToStringConverter"/>

		<!--SpamKeyword converters-->
		<bean class="converters.StringToSpamKeywordConverter"/>
        <bean class="converters.SpamKeywordToStringConverter"/>
    </util:list>	
</beans>
